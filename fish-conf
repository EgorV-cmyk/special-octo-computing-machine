set -gx EDITOR nano
set -xU HOME /mnt/external/0/etc/home
set -xU LAVENDER /mnt/external/0/
function run
    set -l prog $argv[1]

    for dir in /mnt/external/0/bin /mnt/external/0/usr/bin
        set -l path "$dir/$prog"
        if test -x "$path"
            exec /mnt/external/0/lib/ld-musl-x86_64.so.1 \
                --library-path /mnt/external/0/lib:/mnt/external/0/usr/lib:/mnt/external/0/usr/libexec \
                "$path"
            return
        end
    end

    echo (set_color red)"[âœ—] Program '$prog' not found in /mnt/external/0/bin or /mnt/external/0/usr/bin."(set_color normal)
end
